{
  "name": "totara-fix",
  "version": "1.1.1",
  "description": "Totara helper extension",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "./view/MoodFix.html",
    "default_icon": {
      "16": "view/mood16.png",
      "32": "view/mood32.png",
      "48": "view/mood48.png",
      "128": "view/mood128.png"
    }
  },
  "icons": {
    "16": "view/mood16.png",
    "32": "view/mood32.png",
    "48": "view/mood48.png",
    "128": "view/mood128.png"
  },
  "permissions": ["declarativeContent", "tabs", "activeTab", "https://careeracademy.online/"],
  "manifest_version": 2
}